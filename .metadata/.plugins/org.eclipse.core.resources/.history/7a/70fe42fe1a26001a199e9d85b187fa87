package com.hare.krsna;

public class Counter extends Thread {
	int i = 0;

	@Override
	public void run() {
		this.sleep(3);
		this.i = this.i + 2;
	}

	/**
	 * @return the i
	 */
	public int getI() {
		return this.i;
	}

	/**
	 * @param i the i to set
	 */
	public void setI(int i) {
		this.i = i;
	}

}
