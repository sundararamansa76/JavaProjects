package com.hare.krsna.springin5steps.cdi;

public class CdiBus {
	private CdiBusDao cdiDao;

	/**
	 * @return the cdiDao
	 */
	public CdiBusDao getCdiDao() {
		return cdiDao;
	}

	/**
	 * @param cdiDao the cdiDao to set
	 */
	public void setCdiDao(CdiBusDao cdiDao) {
		this.cdiDao = cdiDao;
	}

	
}
