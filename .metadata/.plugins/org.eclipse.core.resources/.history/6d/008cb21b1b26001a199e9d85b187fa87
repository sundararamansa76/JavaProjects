package com.hare.krsna;

public class Counter extends Thread {
	int i = 0;

	@Override
	public void run() {
		try {
			this.sleep(3);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		this.i = this.i + 2;
	}

	/**
	 * @return the i
	 */
	public int getI() {
		return this.i;
	}

	/**
	 * @param i the i to set
	 */
	public void setI(int i) {
		this.i = i;
	}

}
